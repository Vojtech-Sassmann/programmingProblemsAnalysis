def nejdelsi_slovo(text):
    max = 0
    najdlhsie = []
    slova = text.split(" ")
    for i in range (len(slova)):
        if len(slova[i])>=max:
            najdlhsie.append(slova[i])
            max = len(slova[i])
    return 
