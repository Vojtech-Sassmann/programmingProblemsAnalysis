def nejdelsi_slovo(text):
    slova = text.split(" ")
    ds=[]
    n= 1
    for s in slova:
        if len(s) > n:
            ds = []
            n = len(s)
        if len(s) == n:
            ds.append(s)
    ds.sort()
 
