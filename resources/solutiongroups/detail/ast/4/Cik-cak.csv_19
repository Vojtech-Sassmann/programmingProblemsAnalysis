def cikcak(text):
    revtext = text[0::2] + text[1::2]
    dotext = ".".join(revtext)
    if len(dotext)%2 == 1: dotext += "."
    print " ".join(dotext[:len(dotext)/2])
    print " ".join(dotext[len(dotext)/2:])
 
