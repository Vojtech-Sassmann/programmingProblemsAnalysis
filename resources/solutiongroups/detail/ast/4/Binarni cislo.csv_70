def vyhodnot(retezec):
    x=0
    for i in range(1,len(retezec)+1):
        if retezec[len(retezec)-i]==0 or retezec[len(retezec)-i]==1:
            x+=int(retezec[len(retezec)-i])*(2**(i-1))               
    return x
 
