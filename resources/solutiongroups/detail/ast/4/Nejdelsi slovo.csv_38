def nejdelsi_slovo(text):
    slova = text.split(" ")
    slova.sort()
    slova.reverse()
    nejvetsi=slova[0]
    for slovo in slova:
        if len(slovo)>=len(nejvetsi):
            nejvetsi=slovo
    return nejvetsi
 
