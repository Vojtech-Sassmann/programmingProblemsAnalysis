def factorial(m):
    fact = 1
    for i in range(1, m + 1):
        fact = fact * i
    return fact

def KombCislo(n, k):
    citatel = factorial(n)
    jmenovatel = factorial(n-k) * factorial(k)
    komb = citatel/jmenovatel 
