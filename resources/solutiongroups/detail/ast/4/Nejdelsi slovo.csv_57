def nejdelsi_slovo(text):
    slova = text.split(" ")
    slova.sort( lambda x,y: len(y) - len(x))
    i = len(slova[0])
    a = []
    for s in slova:
        if len(s) == i:
            a.append(s)
            
    a.sort()
 
