def nejdelsi_slovo(text):
    slova = text.split(" ")
    nejd= slova[0]
    for x in slova:
        if len(x)>=len(nejd):
            if ord(nejd[0])>ord(x[0]):
                nejd = x
            nejd = x 
    return nejd
 
