def vyhodnot(retezec):
    cislo = 0
    n = len(retezec) - 1
    mocnina = 0
    while n >= 0:
        cislo = cislo + int(retezec[n]) * 2**mocnina
        n = n-1
        mocnina = mocnina +1
    return cislo
 
