def vyhodnot(retezec):
    i=int(retezec)
    kolikatou = -1
    soucet = 0
    while i > 0:
        if i % 10 == 1:
            i = (i - 1)/10
            kolikatou += 1
            soucet = soucet + 2**(kolikatou)
 
