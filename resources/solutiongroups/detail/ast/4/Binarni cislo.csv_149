def vyhodnot(retezec):
    suma = 0
    for i in range (len(retezec)):
        if retezec[len(retezec)-i-1] == "0" or retezec[len(retezec)-i-1] == "1":
            suma += int(retezec[len(retezec)-i-1])*(2**i) 
    return suma
 
