def vyhodnot(retezec):
    sucet = 0
    for i in range(len(retezec)):
        if int(retezec[i]) == 1:
            sucet += 2 ** (len(retezec) -1 -i)
        if int(retezec[i]) == 0:
            sucet += 0
    return sucet
 
