def nejdelsi_slovo(text):
    slovo=text.split(" ")
    for i in range(len(slovo)):
        for j in range(len(slovo)-1-i):
            if len(slovo[j])>len(slovo[j+1]):
                slovo[j],slovo[j+1]=slovo[j+1],slovo[j]
 
